{
  "version": "0.8.0",
  "description": "Rabby's desktop application, for WEB3 users",
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/RabbyHub/RabbyDesktop#readme",
  "bugs": {
    "url": "https://github.com/RabbyHub/RabbyDesktop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RabbyHub/RabbyDesktop.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rabby",
    "url": "https://rabby.io/"
  },
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env-shell NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "npm run clean:renderer && cross-env-shell NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "clean:renderer": "rimraf release/app/dist/renderer && rimraf assets/desktop_shell",
    "preinstall": "npm run install:native",
    "postinstall": "npm run depcheck && npm run install:rabbyX && npm run sync:chain-data",
    "depcheck": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env-shell NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "install:native": "cd release/app && npm install && cd ../",
    "install:rabbyX": "sh scripts/install_rabbyx.sh",
    "lint": "cross-env-shell NODE_ENV=development eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-fix": "cross-env-shell NODE_ENV=development eslint --fix --ext .js,.jsx,.ts,.tsx --quiet ./src",
    "lint-fix:full": "cross-env-shell NODE_ENV=development eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "generate-ico": "png-to-ico assets/icon.png > assets/icon.ico",
    "prepackage": "ts-node ./.erb/scripts/clean.js dist && npm run build",
    "package": "electron-builder build --config=electron-builder.config.js --publish never",
    "package:win32:x64": "rimraf release/build-win32-x64* && cross-env-shell BUILD_ARCH=x64 npm run package",
    "package:win32:ia32": "rimraf release/build-win32-ia32* && cross-env-shell BUILD_ARCH=ia32 npm run package",
    "package:darwin:x64": "rimraf release/build-darwin-x64* && BUILD_ARCH=x64 npm run package",
    "package:darwin:arm64": "rimraf release/build-darwin-arm64* && BUILD_ARCH=arm64 npm run package",
    "postpackage": "node .erb/scripts/rename-autoupdator-zip.js",
    "prepare": "husky install",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env-shell NODE_ENV=development electronmon -r ts-node/register/transpile-only -r tsconfig-paths/register .",
    "start:preload": "cross-env-shell NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "npm run clean:renderer && cross-env-shell NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "sync:chain-data": "cp node_modules/@debank/common/dist/chain-data.js ./src/isomorphic/chain-data.js",
    "test": "jest",
    "pretest:main": "tsc --project ./tsconfig.fixtures.json",
    "test:main": "node ./scripts/spec-runner.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "cross-env-shell NODE_ENV=development eslint --quiet --fix"
    ],
    "src/*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "src/*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "src/*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
